	<select id="loadById" parameterType="java.lang.Integer"
		resultMap="BaseResultMap">
		SELECT
		<include refid="Base_Column_List" />
		FROM tds_user
		WHERE
		ID=#{id}
	</select>

	<select id="selectAll" resultMap="BaseResultMap" parameterType="cn.entity.TdsUser">
		SELECT
		<include refid="Base_Column_List" />
		FROM tds_user
		<where>
			<if test="id != null"> ID = #{id},</if>
			<if test="name != null"> NAME = #{name},</if>
			<if test="comName != null"> COM_NAME = #{comName},</if>
			<if test="comUrl != null"> COM_URL = #{comUrl},</if>
			<if test="contact != null"> CONTACT = #{contact},</if>
			<if test="phone != null"> PHONE = #{phone},</if>
			<if test="password != null"> PASSWORD = #{password},</if>
			<if test="invitedcode != null"> INVITEDCODE = #{invitedcode},</if>
			<if test="remarks != null"> REMARKS = #{remarks},</if>
			<if test="creater != null"> CREATER = #{creater},</if>
			<if test="updater != null"> UPDATER = #{updater},</if>
			<if test="isDeleted != null"> IS_DELETED = #{isDeleted},</if>
			<if test="updateTime != null"> UPDATE_TIME = #{updateTime},</if>
			<if test="createTime != null"> CREATE_TIME = #{createTime},</if>
			<if test="token != null"> TOKEN = #{token},</if>
			<if test="ip != null"> IP = #{ip},</if>
			<if test="lastLoginTime != null"> LAST_LOGIN_TIME = #{lastLoginTime},</if>
			<if test="version != null"> VERSION = #{version},</if>
			<if test="hedehref != null"> HEDEHREF = #{hedehref},</if>
		</where>
	</select>

	<insert id="save" parameterType="cn.entity.TdsUser">
		INSERT INTO tds_user
		<trim prefix="(" suffix=")" suffixOverrides=",">
